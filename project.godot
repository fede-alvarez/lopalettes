; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="RandomSpec"
config/description="Get a random palette or select one you like from lospec.com"
run/main_scene="res://scenes/App.tscn"
boot_splash/image="res://assets/boot_transparent.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.12549, 0.145098, 0.192157, 1 )
config/icon="res://icon.png"

[autoload]

Globals="*res://scripts/Globals.gd"

[display]

window/size/width=430
window/size/height=220
window/size/resizable=false
window/size/borderless=true
window/size/always_on_top=true
window/per_pixel_transparency/allowed=true
window/per_pixel_transparency/enabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
